<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Sero.Functional.Newtonsoft.Json</Title>
    <Version>1.0.0</Version>
    <PackageProjectUrl>https://github.com/Agnael/Sero.Functional.Newtonsoft.Json</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Agnael/Sero.Functional.Newtonsoft.Json</RepositoryUrl>
    <PackageLicenseExpression> Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Sero.Functional" Version="1.0.0" />
  </ItemGroup>
</Project>
